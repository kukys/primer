#include<iostream>
#include<string>
using std::string;
using std::cout;
using std::cin;
using std::endl;
int main()
{
	string a;
	do
	{
		cout<<"请输入两个string对象";
		string v1 ,v2;
		cin>>v1>>v2;
	  string s=v1.size()<=v2.size() ? v1:v2;
	  	cout<<"短字符串是"<<s<<endl;
	  cout<<"还想继续比较吗?"<<endl;
	  cout<<"enter yes or no"<<endl<<endl;;
	  cin>>a;	
	}while(!a.empty()&&tolower(a[0])=='y');
	return 0;
}